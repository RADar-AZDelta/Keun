{
  "name": "keun",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host=127.0.0.1 --port=5179",
    "unlighthouse": "pnpm dlx unlighthouse --site https://radar-azdelta.github.io/Keun/",
    "build": "vite build",
    "preview": "vite build && vite preview --host=127.0.0.1",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "pnpm format && prettier . --check . && eslint . --fix",
    "format": "prettier . --write .",
    "artifactregistry-login": "npx google-artifactregistry-auth"
  },
  "devDependencies": {
    "@browsermt/bergamot-translator": "^0.4.9",
    "@playwright/test": "^1.39.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.2.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/better-sqlite3": "^7.6.7",
    "@types/lodash.debounce": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.0",
    "sass": "^1.69.5",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "svelte-markdown": "^0.4.0",
    "svelte-preprocess": "^5.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-static-copy": "^0.17.0",
    "vitest": "^1.0.0"
  },
  "type": "module",
  "dependencies": {
    "@radar-azdelta/radar-firebase-utils": "^0.0.14",
    "@radar-azdelta/radar-svelte-components": "^0.0.13",
    "@radar-azdelta/radar-utils": "^0.0.6",
    "@radar-azdelta/svelte-athena-search": "^0.0.32",
    "@radar-azdelta/svelte-datatable": "^0.0.67",
    "arquero": "^5.2.0",
    "better-sqlite3": "^9.1.1",
    "firebase": "^10.7.1",
